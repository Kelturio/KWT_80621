DATA_BLOCK "Retain_DB"
{ S7_Optimized_Access := 'TRUE' }
AUTHOR : Searinox
VERSION : 0.1
NON_RETAIN
   VAR RETAIN
      Settings : Struct
         MaxPressureDeltaPerc1 { S7_SetPoint := 'True'} : Int;   // Maximum pressure difference in percent before error
         MaxPressureDeltaPerc2 { S7_SetPoint := 'True'} : Int;   // Maximum pressure difference in percent before error
         MaxTempTransgression { S7_SetPoint := 'True'} : Real;   // Maximum temperature overrun of setpoint in centigrade before error
         SprayTubeOpenTimeout { S7_SetPoint := 'True'} : Time;   // Maximum time until spray tube must be open before error
      END_STRUCT;
   END_VAR


BEGIN
   Settings.MaxPressureDeltaPerc1 := 50;
   Settings.MaxPressureDeltaPerc2 := 50;
   Settings.MaxTempTransgression := 4.0;
   Settings.SprayTubeOpenTimeout := T#15s;

END_DATA_BLOCK

